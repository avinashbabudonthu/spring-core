<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="student1" class="autowire.constructor.Student" p:id="1" p:name="jill" />
	
	<bean name="studentRepositoryImpl" class="autowire.constructor.StudentRepositoryImpl" autowire="constructor" />
	
	<bean name="studentServiceImpl" class="autowire.constructor.StudentServiceImpl" autowire="constructor" />

</beans>
